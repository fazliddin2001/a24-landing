                                <div class="p-3 bg-gray-50 rounded-lg">
                                    <p class="text-content-dark text-base font-bold mb-1">
                                        <%= step_connect_title %>
                                    </p>
                                    <p class="text-gray-500 text-sm">
                                        <%= step_connect_desc %>
                                    </p>
                                </div>
