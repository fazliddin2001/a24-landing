                            <span class="material-symbols-outlined text-5xl text-primary mb-2">agriculture</span>
                            <h3 class="font-bold text-lg text-content-dark">
                                <%= tab_agriculture %>
                            </h3>
                        </label>
                    </div>
                </div>
            </div>
            <div class="layout-container flex h-full grow flex-col">
