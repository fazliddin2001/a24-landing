<body class="bg-background-light font-display text-content-dark overflow-x-hidden">
    <div class="relative flex h-auto min-h-screen w-full flex-col overflow-x-hidden">
        <header class="bg-background-dark">
            <div class="layout-container flex h-full grow flex-col max-w-[960px] mx-auto">
                <div class="flex flex-1 justify-center px-4 sm:px-10 md:px-20 lg:px-0">
                    <div class="layout-content-container flex flex-col flex-1">
                        <div
                            class="flex items-center justify-between border-b border-solid border-b-[#393328] px-4 sm:px-10 py-3">
                            <div class="flex items-center gap-4 text-white">
                                <h2 class="text-white text-lg font-bold leading-tight tracking-[-0.015em]">A24</h2>
                            </div>
                            <div class="flex items-center gap-4">
                                <div class="relative group">
                                    <button
                                        class="flex max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-10 bg-[#393328] text-white gap-2 text-sm font-bold leading-normal tracking-[0.015em] min-w-0 px-2.5">
                                        <span class="material-symbols-outlined text-xl">language</span>
                                        <%= current_lang_label %>
                                    </button>
                                    <div
                                        class="absolute right-0 mt-2 w-48 bg-white rounded-md shadow-lg z-10 hidden group-focus-within:block">
                                        <a class="block px-4 py-2 text-sm text-content-dark hover:bg-gray-100"
                                            href="/uz">UZ</a>
                                        <a class="block px-4 py-2 text-sm text-content-dark hover:bg-gray-100"
                                            href="/en">EN</a>
                                        <a class="block px-4 py-2 text-sm text-content-dark hover:bg-gray-100"
                                            href="/ru">RU</a>
                                    </div>
                                </div>
                                <a class="flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-10 px-4 bg-primary hover:brightness-110 transition-all text-[#181511] text-sm font-bold leading-normal tracking-[0.015em]"
                                    href="#connect">
                                    <span class="truncate">
                                        <%= nav_free_ai %>
                                    </span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <main>
            <div class="flex flex-col md:flex-row">
                <div class="layout-container flex h-full grow flex-col md:w-1/2">
                    <div class="flex flex-1 justify-center py-5 px-4 sm:px-10 md:px-20 lg:px-40">
                        <div class="layout-content-container flex flex-col max-w-[960px] flex-1">
                            <div class="py-8 sm:py-16 md:py-24">
                                <div class="p-4">
                                    <div
                                        class="flex min-h-[480px] flex-col gap-6 items-center justify-center p-4 text-center md:items-start md:text-left">
                                        <input checked="" class="hidden" id="hero-select-default" name="hero-select"
                                            type="radio" />
                                        <input class="hidden" id="hero-select-business" name="hero-select"
                                            type="radio" />
                                        <input class="hidden" id="hero-select-education" name="hero-select"
                                            type="radio" />
                                        <input class="hidden" id="hero-select-medicine" name="hero-select"
                                            type="radio" />
                                        <input class="hidden" id="hero-select-agriculture" name="hero-select"
                                            type="radio" />
                                        <div id="hero-text-content">
                                            <div id="hero-text-default">
                                                <div class="flex flex-col gap-4">
                                                    <h1
                                                        class="text-content-dark text-4xl font-black leading-tight tracking-[-0.033em]">
                                                        <%= hero_title %>
                                                    </h1>
                                                    <p
                                                        class="text-gray-500 text-base font-normal leading-normal max-w-xl mx-auto md:mx-0">
                                                        <%= hero_desc %>
                                                    </p>
                                                </div>
                                            </div>
                                            <div id="hero-text-business">
