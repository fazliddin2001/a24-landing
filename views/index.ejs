                                <div class="flex sm:flex-col items-center gap-4 sm:gap-1 pt-3">
                                    <div class="text-primary"><span
                                            class="material-symbols-outlined text-3xl">handshake</span></div>
                                    <div class="w-full sm:w-[1.5px] bg-gray-200 h-[1.5px] sm:h-2 grow"></div>
                                </div>
                                <div class="flex flex-1 flex-col py-3 min-w-0 break-words">
                                    <p class="text-content-dark text-lg font-medium leading-normal">
                                        <%= step_connect_title %>
                                    </p>
                                    <p class="text-gray-500 text-base font-normal leading-normal">
                                        <%= step_connect_desc %>
                                    </p>
                                </div>
                                <div class="flex sm:flex-col items-center gap-4 sm:gap-1">
                                    <div class="w-full sm:w-[1.5px] bg-gray-200 h-[1.5px] sm:h-2"></div>
                                    <div class="text-primary"><span
                                            class="material-symbols-outlined text-3xl">science</span></div>
                                    <div class="w-full sm:w-[1.5px] bg-gray-200 h-[1.5px] sm:h-2 grow"></div>
                                </div>
                                <div class="flex flex-1 flex-col py-3 min-w-0 break-words">
                                    <p class="text-content-dark text-lg font-medium leading-normal">
                                        <%= step_research_title %>
                                    </p>
                                    <p class="text-gray-500 text-base font-normal leading-normal">
                                        <%= step_research_desc %>
                                    </p>
                                </div>
                                <div class="flex sm:flex-col items-center gap-4 sm:gap-1">
                                    <div class="w-full sm:w-[1.5px] bg-gray-200 h-[1.5px] sm:h-2"></div>
                                    <div class="text-primary"><span
                                            class="material-symbols-outlined text-3xl">rocket_launch</span></div>
                                    <div class="w-full sm:w-[1.5px] bg-gray-200 h-[1.5px] sm:h-2 grow"></div>
                                </div>
                                <div class="flex flex-1 flex-col py-3 min-w-0 break-words">
                                    <p class="text-content-dark text-lg font-medium leading-normal">
                                        <%= step_deploy_title %>
                                    </p>
                                    <p class="text-gray-500 text-base font-normal leading-normal">
                                        <%= step_deploy_desc %>
                                    </p>
                                </div>
                                <div class="flex sm:flex-col items-center gap-4 sm:gap-1 pb-3">
                                    <div class="w-full sm:w-[1.5px] bg-gray-200 h-[1.5px] sm:h-2"></div>
                                    <div class="text-primary"><span
                                            class="material-symbols-outlined text-3xl">all_inclusive</span></div>
                                </div>
                                <div class="flex flex-1 flex-col py-3 min-w-0 break-words">
                                    <p class="text-content-dark text-lg font-medium leading-normal">
                                        <%= step_more_title %>
                                    </p>
                                    <p class="text-gray-500 text-base font-normal leading-normal">
                                        <%= step_more_desc %>
                                    </p>
                                </div>
                            </div>
                        </section>
                        <section class="relative py-8 sm:py-16 md:py-24 overflow-x-hidden">
                            <label class="fixed inset-0 z-10 hidden peer-checked/wwd:block bg-black/10"
                                for="close-overlay" id="wwd-overlay"></label>
                            <h2
                                class="text-content-dark text-xl sm:text-[22px] font-bold leading-tight tracking-[-0.015em] pb-6 pt-0 text-center">
                                <%= what_do %>
                            </h2>
                            <div
                                class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 gap-x-2 gap-y-8 overflow-x-hidden">
                                <div class="col-span-1">
                                    <input class="peer/wwd hidden" id="toggle-cloud" name="what-we-do" type="radio" />
