                                        class="hidden peer-checked/wwd:block mt-4 p-4 bg-gray-50 rounded-lg border border-gray-200 text-left break-words">
                                        <h4 class="font-bold text-lg mb-2 text-content-dark">
                                            <%= service_more %>
                                        </h4>
                                        <p class="text-gray-600">
                                            <%= service_more_desc %>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            <script>
                                // Uncheck radio buttons when clicking outside the section
                                document.addEventListener('DOMContentLoaded', function () {
                                    const overlay = document.getElementById('wwd-overlay');
                                    const radios = document.querySelectorAll('input[name="what-we-do"]');
                                    overlay.addEventListener('click', function () {
                                        radios.forEach(radio => radio.checked = false);
                                    });
                                });
                            </script>
                        </section>
                        <section class="py-8 sm:py-16 md:py-24 text-center overflow-x-hidden" id="connect">
                            <h2
                                class="text-content-dark text-[22px] font-bold leading-tight tracking-[-0.015em] pb-4 pt-0 text-center sm:block">
                                <%= connect_title %>
                            </h2>
